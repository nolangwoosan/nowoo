{
  "name": "nowoo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm dev:db:setup && next dev",
    "dev:db:setup": "pnpm generate:client",
    "dev:db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "dev:db:pull": "dotenv -e .env.local -- prisma db pull",
    "dev:db:push": "dotenv -e .env.local -- prisma db push",
    "dev:db:reset": "dotenv -e .env.local -- pnpm db:reset",
    "dev:db:studio": "dotenv -e .env.local -- prisma studio",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:db:setup": "pnpm db:start && dotenv -e .env.test -- pnpm db:deploy && pnpm generate:client",
    "test:db:reset": "dotenv -e .env.test -- pnpm db:reset",
    "db:reset": "prisma migrate reset -f",
    "db:deploy": "prisma migrate deploy",
    "generate:client": "prisma generate",
    "build": "next build",
    "start": "next start",
    "start:dev": "pnpm dev:db:setup && env-cmd -f .env.development next build && next start",
    "start:prod": "env-cmd -f .env.development next build && next start",
    "lint": "next lint && biome lint .",
    "test": "vitest run",
    "format": "prettier -w . && biome format --write .",
    "check:spell": "cspell '**' --gitignore --no-progress",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.4.0",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-query-devtools": "^5.17.21",
    "argon2": "^0.31.2",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "lucide-react": "^0.314.0",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "nodemailer": "^6.9.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "tailwind-merge": "^2.2.1",
    "use-debounce": "^10.0.0",
    "valibot": "^0.27.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@mermaid-js/mermaid-cli": "^10.8.0",
    "@playwright/test": "^1.41.2",
    "@prisma/client": "5.10.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/eslint-plugin-query": "^5.17.22",
    "@testing-library/react": "^14.2.1",
    "@types/gtag.js": "^0.0.18",
    "@types/node": "^20.11.8",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cspell": "^8.4.1",
    "dotenv-cli": "^7.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.7",
    "playwright": "^1.41.2",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "prisma": "^5.10.2",
    "prisma-erd-generator": "^1.11.2",
    "supabase": "^1.136.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
